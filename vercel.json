{
  "buildCommand": "bash ./build.sh",
  "outputDirectory": "ui/dist",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/ui/dist/$1"
    }
  ]
}
