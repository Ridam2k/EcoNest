{
    "buildCommand": "cd ui && npm install && npm run build",
    "outputDirectory": "ui/dist",
    "framework": "vite",
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/app.py"
      }
    ],
    "functions": {
      "api/app.py": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }
  
  